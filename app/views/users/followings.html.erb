<h1>followings:</h1>
<table>
  <tr>
    <th>name</th>
    <th>age</th>
    <th>sex</th>
    <th colspan="3"></th>
  </tr>
<% @followings.each do |following| %>
    <tr>
      <td><%= following.name %></td>
      <td><%= following.age %></td>
      <td><%= following.sex %></td>
    </tr>
  <% end %>
</table>